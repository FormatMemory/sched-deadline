[project]
	description = An implementation of the popular Earliest Deadline First (EDF) scheduling algorithm or the Linux kernel. It is still experimental code, and it is still continuously being improved, but it already is a fully-working solution for supporting typical real-time applications in GNU/Linux environments.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Yusheng Ding (FormatMemory)
[access "refs/for/refs/*"]
	read = group user/Yusheng Ding (FormatMemory)
	push = group user/Yusheng Ding (FormatMemory)
	label-Code-Review = -2..+2 group user/Yusheng Ding (FormatMemory)
	label-Verified = -1..+1 group user/Yusheng Ding (FormatMemory)
	rebase = group user/Yusheng Ding (FormatMemory)
	removeReviewer = group user/Yusheng Ding (FormatMemory)
	submit = group user/Yusheng Ding (FormatMemory)
[access "refs/heads/*"]
	read = group user/Yusheng Ding (FormatMemory)
	create = group user/Yusheng Ding (FormatMemory)
	push = +force group user/Yusheng Ding (FormatMemory)
	pushMerge = group user/Yusheng Ding (FormatMemory)
[access "refs/tags/*"]
	push = +force group user/Yusheng Ding (FormatMemory)
	pushTag = group user/Yusheng Ding (FormatMemory)
	pushSignedTag = group user/Yusheng Ding (FormatMemory)
